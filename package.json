{
  "name": "nextstage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:next dev:path",
    "devAll": "npm-run-all --parallel dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --ignorePath .gitignore --watch",
    "dev:sb": "start-storybook -p 6006",
    "build": "npm-run-all build:path build:next build:sitemap",
    "buildAll": "npm-run-all build:*",
    "build:next": "next build",
    "build:path": "pathpida --ignorePath .gitignore",
    "build:sb": "build-storybook ",
    "build:sitemap": "next-sitemap",
    "start": "next start",
    "serve": "npm-run-all build start",
    "lint": "npm-run-all next:lint eslint format",
    "lint:fix": "npm-run-all next:lint eslint:fix format:fix",
    "next:lint": "next lint",
    "unit": "jest",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:install": "playwright install",
    "e2e:codegen": "playwright codegen",
    "prepare": "husky install",
    "checkLicense": "license-checker --production",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "postInstall": "typesync"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "next": "12.1.0",
    "next-seo": "^5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "sass": "^1.49.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@playwright/test": "^1.20.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.1",
    "@types/license-checker": "^25.0.3",
    "@types/node": "17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-access": "^1.0.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.3.7",
    "next-sitemap": "^2.5.13",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^10.0.0",
    "storybook-addon-sass-postcss": "^0.1.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.2",
    "typesync": "^0.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix .",
      "prettier --write ."
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write ."
    ]
  }
}
