{
  "name": "nextstage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "serve": "npm-run-all build start",
    "lint": "next lint",
    "unit": "jest",
    "prepare": "husky install",
    "checkLicense": "license-checker --production",
    "format": "prettier --check .",
    "postInstall": "typesync"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/license-checker": "^25.0.3",
    "@types/node": "17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import-access": "^1.0.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "4.6.2",
    "typesync": "^0.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix .",
      "prettier --write ."
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write ."
    ]
  }
}
