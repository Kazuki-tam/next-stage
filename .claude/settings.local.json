{
  "permissions": {
    "allow": [
      "Bash(bun install:*)",
      "Bash(bun run:*)"
    ],
    "deny": [
      "Read(.env)",
      "Write(.env)",
      "Read(.env.*)",
      "Write(.env.*)",
      "Read(**/*token*)",
      "Read(**/*key*)",
      "Bash(sudo:*)",
      "Bash(rm -rf /)",
      "Bash(rm -rf ~)",
      "Bash(rm -rf ~/*)",
      "Bash(rm -rf /*)",
      "Bash(curl:*)",
      "Bash(wget:*)",
      "Bash(nc:*)",
      "Bash(psql:*)",
      "Bash(mysql:*)",
      "Bash(mongod:*)"
    ]
  }
}
